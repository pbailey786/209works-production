{
  "name": "209jobs-next14",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "cross-env OTEL_SDK_DISABLED=true npx prisma generate && cross-env OTEL_SDK_DISABLED=true next build",
    "build:no-lint": "next build --no-lint",
    "build:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"npm run build\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "start": "next start",
    "lint": "next lint",
    "lint:watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"npm run lint\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "security:scan": "node -e \"require('./src/lib/security/dependency-scanner.ts').runSecurityScan()\"",
    "security:scan:npm": "npm audit --json",
    "security:scan:ci": "node -e \"require('./src/lib/security/dependency-scanner.ts').runSecurityScan({ failOnVulnerabilities: true })\"",
    "security:fix": "npm audit fix",
    "security:generate-key": "node -e \"console.log('ENCRYPTION_KEY=' + require('./src/lib/encryption.ts').generateEncryptionKey())\"",
    "security:check-config": "node -e \"require('./src/lib/encryption.ts').validateEncryptionEnvironment(); require('./src/lib/security/security-monitor.ts').validateSecurityMonitoringEnvironment();\"",
    "setup:company-knowledge": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/setup-company-knowledge.ts",
    "test:phase2-chatbot": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/test-phase2-chatbot.ts",
    "deploy:dev": "bash scripts/deploy.sh deploy development",
    "deploy:staging": "bash scripts/deploy.sh deploy staging",
    "deploy:prod": "bash scripts/deploy.sh deploy production",
    "deploy:status": "bash scripts/deploy.sh status",
    "deploy:rollback:dev": "bash scripts/deploy.sh rollback development",
    "deploy:rollback:staging": "bash scripts/deploy.sh rollback staging",
    "deploy:rollback:prod": "bash scripts/deploy.sh rollback production",
    "deploy:health": "bash scripts/deploy.sh health",
    "vercel:env:pull": "vercel env pull .env.local",
    "vercel:env:push": "vercel env push .env.local",
    "cron:start": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/start-cron-scheduler.ts start",
    "cron:stop": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/start-cron-scheduler.ts stop",
    "cron:status": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/start-cron-scheduler.ts status",
    "cron:test": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/start-cron-scheduler.ts test",
    "queue:status": "curl -H \"Authorization: Bearer ${ADMIN_SECRET:-${CRON_SECRET}}\" http://localhost:3000/api/email-queue",
    "queue:pause": "curl -X POST -H \"Authorization: Bearer ${ADMIN_SECRET:-${CRON_SECRET}}\" \"http://localhost:3000/api/email-queue?action=pause\"",
    "queue:resume": "curl -X POST -H \"Authorization: Bearer ${ADMIN_SECRET:-${CRON_SECRET}}\" \"http://localhost:3000/api/email-queue?action=resume\"",
    "queue:clear": "curl -X POST -H \"Authorization: Bearer ${ADMIN_SECRET:-${CRON_SECRET}}\" \"http://localhost:3000/api/email-queue?action=clear\"",
    "queue:test": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/test-email-queue.ts"
  },
